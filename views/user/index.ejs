
<h3>Gebruikers</h3>
		<table class="table table-striped table-bordered table-condensed">
			<thead>
				<tr>
					<th>Gebruikersnaam</th>
					<th>Naam</th>
					<th>Organisatie</th>					
					<th>Rol</th>
					<th>Datum aangemaakt</th>
					<th>Bewerken</th>
				</tr>
			</thead>
			<tbody>
				<% accounts.forEach(function(account){ %>
					<tr>
						<td><a href="/<%=account.username%>" target="_blank" class="not_ajax"><%=account.username%></a></td>
						<td><%=account.firstName%> <%=account.lastName%></td>
						<td><%=account.organisatie%></td>
						<td><%=account.rol%></td>
						<td><%=account.createdAt%></td>
						<td><a href=/user/edit/<%=account.username%>>bewerken</a></td>
					</tr>
				<% }) %>
			</tbody>
		</table>